/* struct Node
 {
   int data;
   Node* next;
 }; */

/* Should return data of n'th node from the end of linked list */
int getNthFromLast(Node *head, int n)
{
      if(head == NULL)
        return -1;
      Node* temp = head;
      Node* ref = head;
      
      while(n!=0)
      {
          if(temp->next!=NULL)
          {
              temp = temp->next;
          }
         n--;
      }
      
      while(temp!=NULL)
      {
          temp = temp->next;
          ref = ref->next;
      }
      return ref->data;
      
}
